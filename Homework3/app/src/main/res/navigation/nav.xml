<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav"
    app:startDestination="@id/listFragment2">
    <fragment
        android:id="@+id/listFragment2"
        android:name="com.example.homework3.ListFragment"
        android:label="ListFragment" >
        <action
            android:id="@+id/action_listFragment2_to_selectedSongFragment"
            app:destination="@id/selectedSongFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/selectedSongFragment"
        android:name="com.example.homework3.SelectedSongFragment"
        android:label="fragment_selected_song"
        tools:layout="@layout/fragment_selected_song" />
</navigation>